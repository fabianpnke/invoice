{
    "name": "fabianpnke/invoice",
    "description": "ADM Invoice Library",
    "type": "library",
    "version: "1.0.1",
    "license": "GPL-2.0-or-later",
    "keywords": ["invoice", "pdf", "adm", "fabianpnke"],
    "authors": [
        {
            "name": "Fabian Pankoke",
            "homepage": "https://github.com/fabianpnke"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": { "Fabianpnke\\Invoice\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabianpnke\\Invoice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
